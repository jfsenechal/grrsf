{% set classDay = "" %}
{% set daySelected = app.request.get('day') %}

{% if daySelected == day.day %}
    {% set classDay = "table-info" %}
{% endif %}

<td class="{{ classDay }}">
    {% if day.month == firstDay.month %}
        <a href="{{ generateRouteDayView(day.day) }}">{{ day.day }}</a>
    {% endif %}
</td>