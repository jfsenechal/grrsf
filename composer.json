    "symfony/http-client": "4.3.*",
    "symfony/intl": "4.3.*",
    "symfony/ldap": "4.3.*",
    "symfony/mailer": "^4.3",
    "symfony/messenger": "4.3.*",
    "symfony/mime": "^4.3",
